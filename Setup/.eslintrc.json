{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "indent": ["error", 2],
    "no-multi-spaces": "error",
    "no-trailing-spaces": "error",
    "no-irregular-whitespace": "error"
  }
}
